		"svelte": "^5.2.0",
		"svelte-check": "^4.0.8",
		"tailwindcss": "^3.4.15",
		"typescript": "^5.6.3",
		"typescript-eslint": "^8.14.0",
		"vite": "^5.4.11",
		"vitest": "^2.1.5"
	},
	"dependencies": {
		"@inlang/paraglide-sveltekit": "^0.11.5",
		"@libsql/client": "^0.9.0",
		"@node-rs/argon2": "^1.8.3",
		"@oslojs/crypto": "^1.0.1",
		"@oslojs/encoding": "^1.1.0",
		"@tailwindcss/aspect-ratio": "^0.4.2",
		"@tailwindcss/container-queries": "^0.1.1",
		"@tailwindcss/forms": "^0.5.9",
